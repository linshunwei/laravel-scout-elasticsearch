{
    "name": "linshunwei/laravel-scout-elasticsearch",
    "type": "library",
    "description": "The Elasticsearch Driver for Laravel Scout",
    "keywords": [
        "elastic",
        "elasticsearch",
        "driver",
        "engine",
        "laravel",
        "scout",
        "search"
    ],
    "homepage": "https://github.com/linshunwei/laravel-scout-elasticsearch",
    "license": "MIT",
    "authors": [
        {
            "name": "linshunwei",
            "email": "894584225@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "elasticsearch/elasticsearch": "6.*",
        "laravel/scout": "7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*",
        "mockery/mockery": "1.*"
    },
    "autoload": {
        "psr-4": {
            "ScoutElastic\\Tests\\": "tests/",
            "ScoutElastic\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ScoutElastic\\ScoutElasticServiceProvider"
            ]
        }
    }
}
